{
    "name": "hashcrypt/export",
    "description": "Laravel Package to export data to Excel CSV",
    "keywords": [
        "hashcrypt",
        "laravel",
        "export",
        "csv",
        "excel"
    ],
    "authors": [
        {
            "name": "HashCrypt Developers",
            "email": "hashcryptdevs@gmail.com"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "illuminate/auth": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "illuminate/container": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "illuminate/contracts": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "illuminate/database": "~5.4.0|~5.5.0|~5.6.0|~5.7.0"
    },
    "minimum-stability": "stable",
    "prefer-stable" : true,
    "extra": {
        "laravel": {
            "providers": [
                "hashcrypt\\export\\ExportProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "hashcrypt\\export\\": "src/"
        }
    }
}
